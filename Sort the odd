package Level7;

public class Sorttheodd {

	public static int[] sortArray (int[] array){
	
		for(int i =0;i<array.length;i++){
			
			
			if(array[i] %2 != 0){
			for(int a = 1+i;a<array.length;a++){
				if(array[a] %2 != 0){
					if(array[i] > array[a]){
					int temp = array[i];
					array[i] = array[a];
					array[a] = temp;
					}
				}
			}
		}
		}
		
		
	
	return array;
	
	}
	
}
