class Arge {
    
    public static int nbYear(int p0, double percent, int aug, int p) {
      	int z채hler = 0;
    	double cpop = p0;
    
    	do{
        int inc = (int) (cpop*(percent/100));
    		cpop = cpop + aug + inc ;
    		z채hler = z채hler +1;
    	}
    	while(cpop<=p);
    	
    	int ergebnis = z채hler;
    	return ergebnis;

    }
}


